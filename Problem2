#problem2

def get_tshirt(size):
    def get_name():
        n = input()
        return n
    user_name = get_name()
    
    brands = ["Adidas","zudio","zara"]
    brand_want = input()
    if brand_want in brands:
        print("Hi, ", user_name , "the brand you are looking for is available in",size, "size in our store")
    else:
        print("Hi," , user_name , "unfortunately the brand you are looking for is not available in",size, "our store")

Size=input()
get_tshirt(Size)

